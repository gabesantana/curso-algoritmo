programa {
  funcao inicio() {
    //condicional alinhada//
    inteiro n1,n2,m
    escreva("digite a primeira nota ")
      leia(n1)
    escreva("digite a segunda nota ")
      leia(n2)
        m=(n1 + n2) / 2
    escreva(" sua média é ", m)

    se  (m >= 7 ) 
      escreva(" aluno aprovado ")
    senao
        escreva (" aluno reprovado")
        se m >=7 e m<=7
        escreva("e esta de recuperação ")


  }
}
